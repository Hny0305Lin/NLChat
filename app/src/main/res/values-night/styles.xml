<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="HaohanyhDialog" parent="Theme.AppCompat.Dialog">
        <!--让提示框透明，FEC0CB粉色主色，32百分比透明度-->
        <item name="android:windowBackground">#32FEC0CB</item>
    </style>
    <style name="ConsoleDialog" parent="Theme.AppCompat.Dialog">
        <!--让提示框透明，007FFC蓝色主色，48百分比透明度-->
        <item name="android:windowBackground">#48007FFC</item>
    </style>
    <style name="MyCheckBox" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">@color/blue_biaozhun_logowai</item>
        <item name="colorControlActivated">@color/Pink_is_fancy</item>
    </style>
    <style name="MyRadiobutton" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">@color/nearlinkgreen</item>
        <item name="colorControlActivated">@color/blue_biaozhun_logowai</item>
    </style>

    <!--夜间模式适配-->
    <style name="CardViewNight">
        <item name="cardBackgroundColor">@color/cardview_dark_background</item>
        <item name="cardCornerRadius">4dp</item>
        <item name="cardElevation">18dp</item>
    </style>

</resources>